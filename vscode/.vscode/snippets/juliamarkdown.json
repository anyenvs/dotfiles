    // 	"body": [
    // 		"console.log('$1');",
    // 		"$2"
    // 	],
    // 	"description": "Log output to console"
    // }

    // Markdown
    "markdown images expanding": {
        "prefix": "markdown-images-expanding",
        "body": [
            "<details>",
            "<summary>Click to expand details with image</summary>\n",
            "![image info](img/01-CICD-github-error-check.png)",
            "</details>",
        ],
        "description": "Custom snippet for HTTP call "
    },
    //
    "markdown images": {
        "prefix": "markdown-images",
        "body": [
            "[image info](img/01-CICD-github-error-check.png)",
        ],
        "description": "Custom snippet for HTTP call "
    },
}
