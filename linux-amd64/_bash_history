opkg list-installed
opkg list | grep openrc
. ~/.anyenvrc
nodenv install -l
TMPDIR=${DATA_1}/tmp nodenv install 16.9.0 ; nodenv global 16.9.0
TMPDIR=${DATA_1}/tmp nodenv install 19.0.0 ; nodenv global 19.0.0
nodenv versions
. ~/.*env/completions/*.${SHELL##*/} &>/dev/null ; echo $?
cd /mnt/HD/HD_a2/DATA_1/opt/repos/_dotfiles/ ; bash install.sh linux-armv7
cd /mnt/HD/HD_a2/DATA_1/opt/repos/_dotfiles/deploy/proxy/ ;
docker-compose -f ./docker-compose.armv7l.yml config --services
docker-compose -f ./docker-compose.armv7l.yml build confd proxy
docker-compose -f ./docker-compose.armv7l.yml run -it --rm confd
docker-compose -f ./docker-compose.armv7l.yml run -it --rm proxy
docker-compose -f ./docker-compose.armv7l.yml run --build -d
docker-compose -f ./docker-compose.armv7l.yml up --build -d
docker-compose -f ./docker-compose.armv7l.yml exec -it proxy bash
docker-compose -f ./docker-compose.armv7l.yml down --remove-orphans
git fetch
git reset --hard origin/main

